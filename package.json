{
  "name": "waitlist-website",
  "version": "1.0.0",
  "description": "정적 대기자 명단 웹사이트",
  "scripts": {
    "start": "parcel src/pages/index.html",
    "build": "parcel build src/pages/*.html --dist-dir dist --public-url ./",
    "dev": "parcel src/pages/index.html --open",
    "editor": "parcel src/pages/editor.html --open",
    "sitemap": "parcel src/pages/sitemap.html --open",
    "dev:all": "parcel src/pages/*.html",
    "clean": "rm -rf dist .parcel-cache"
  },
  "dependencies": {
    "grapesjs": "^0.21.13",
    "grapesjs-preset-webpage": "^1.0.3"
  },
  "devDependencies": {
    "parcel": "^2.12.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
